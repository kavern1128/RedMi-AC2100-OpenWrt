CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7621=y
CONFIG_TARGET_ramips_mt7621_DEVICE_xiaomi_redmi-router-ac2100=y

# IPv6支持:
CONFIG_PACKAGE_6in4=y
CONFIG_PACKAGE_ipv6helper=y

CONFIG_PACKAGE_luci-app-wolplus=n # 网络唤醒
CONFIG_PACKAGE_luci-app-ddns=n # ddns
CONFIG_PACKAGE_luci-app-advanced=n # 高级设置
CONFIG_PACKAGE_luci-app-upnp=n # upnp
CONFIG_PACKAGE_luci-app-vlmcsd=n # kms的luci控制面板
CONFIG_PACKAGE_luci-app-arpbind=n # ip/mac绑定(arp绑定)
CONFIG_PACKAGE_luci-app-bypass=n # 某代理
CONFIG_PACKAGE_luci-app-openclash=n # openclash
CONFIG_PACKAGE_luci-app-wireguard=n # wireguard
CONFIG_PACKAGE_luci-app-netdata=n # 实时监控
CONFIG_PACKAGE_luci-app-ttyd=n # 网页终端
CONFIG_PACKAGE_luci-app-ramfree=n # 释放内存
CONFIG_PACKAGE_luci-app-openclash=y

# 禁止软件源，因为我使用的自定义软件源会出现在编译后的固件里导致报错。
CONFIG_FEED_custom=y

# 主题
CONFIG_PACKAGE_luci-theme-argon=y
CONFIG_PACKAGE_luci-app-argon-config=y

# dnsmasq
CONFIG_PACKAGE_dnsmasq=n
CONFIG_PACKAGE_dnsmasq-full=y
CONFIG_PACKAGE_dnsmasq_full_dhcp=y
CONFIG_PACKAGE_dnsmasq_full_ipset=y

# CONFIG_PACKAGE_luci-ssl=n # uhttpd服务
CONFIG_PACKAGE_luci-base=y
CONFIG_PACKAGE_luci-compat=y

CONFIG_LUCI_LANG_zh_Hans=y #中文语言包

# CONFIG_PACKAGE_coremark=y # CPU跑分
# CONFIG_COREMARK_OPTIMIZE_O3=y
# CONFIG_COREMARK_ENABLE_MULTITHREADING=y
# CONFIG_COREMARK_NUMBER_OF_THREADS=16

# linux软件包
# CONFIG_PACKAGE_curl=y
# CONFIG_PACKAGE_htop=y
# CONFIG_PACKAGE_nano=y
# CONFIG_PACKAGE_bash=y

# CONFIG_PACKAGE_zram-swap=y # 内存压缩
CONFIG_boost-compile-visibility-hidden=y
CONFIG_boost-runtime-shared=y
CONFIG_boost-static-and-shared-libs=y
CONFIG_boost-variant-release=y
